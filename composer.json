{
    "name": "bitpress/blade-extensions",
    "description": "Laravel Blade extension classes",
    "keywords": [
        "laravel",
        "blade",
        "bitpress"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Paul Redmond",
            "email": "paul@bitpress.io"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0|~5.8.0",
        "illuminate/console": "~5.5.0|~5.6.0|~5.7.0|~5.8.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.5.0|~3.6.0|~3.7.0",
        "phpunit/phpunit": "^6.4|^7.0"
    },
    "autoload": {
        "psr-4": {
            "BitPress\\BladeExtension\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "BitPress\\BladeExtension\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BitPress\\BladeExtension\\BladeExtensionServiceProvider"
            ]
        }
    },
    "prefer-stable": true
}
